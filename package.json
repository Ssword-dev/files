{
  "name": "q",
  "packageManager": "yarn@4.6.0",
  "scripts": {
    "dev": "concurrently \"yarn tw-watch\" \"yarn hrs\"",
    "hrs": "yarn node scripts/hot-reload.js",
    "tw-watch": "tailwindcss -i commons/tailwind.css -o commons/tailwind-dist.css --watch",
    "tw": "tailwindcss --config ./tailwind.config.js -i commons/tailwind.css -o commons/tailwind-dist.css"
  },
  "devDependencies": {
    "@tailwindcss/cli": "4.1.8",
    "ansi-styles": "^6.2.1",
    "chalk": "^5.4.1",
    "change-case": "^5.4.4",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "strip-json-comments": "^5.0.2",
    "tailwindcss": "4.1.8",
    "typescript": "^5.8.3",
    "ws": "^8.18.3"
  }
}
